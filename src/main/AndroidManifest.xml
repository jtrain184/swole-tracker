<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="swole.swoletracker">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".getWimps"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".createWimp"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".modifyWimpInfo"
            android:parentActivityName=".getWimps"/>
        <activity android:name=".wimpInformation"
            android:parentActivityName=".getWimps"/>
        <activity android:name=".workoutInformation"
            android:parentActivityName=".getWimps"/>
        <activity android:name=".createWorkout"
            android:parentActivityName=".getWimps"/>
        <activity android:name=".modifyWorkoutInfo"
            android:parentActivityName=".getWimps"/>

    </application>

</manifest>